{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": false,
        "blocks": {
            "core/button": {
                "border": {
                    "radius": false
                },
                "color": {
                    "background": false
                }
            },
            "core/image": {
                "lightbox": {
                    "enabled": false,
                    "allowEditing": false
                }
            }
        },
        "border": {
            "color": false,
            "radius": false,
            "style": false,
            "width": false
        },
        "color": {
            "background": true,
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": [
                {
                    "name": "Dark Grey",
                    "slug": "darkgrey",
                    "color": "#1D1D1D"
                },
                {
                    "name": "Light Grey",
                    "slug": "lightgrey",
                    "color": "#232323"
                },
                {
                    "name": "Blue",
                    "slug": "blue",
                    "color": "#00ade9"
                }
            ],
            "text": false
        },
        "layout": {
            "allowEditing": false,
            "contentSize": "765px",
            "wideSize": "1400px"
        },
        "shadow": {
            "defaultPresets": false,
            "presets": []
        },
        "spacing": {
            "blockGap": null,
            "customSpacingSize": false,
            "margin": false,
            "padding": false,
            "units": []
        },
        "typography": {
            "customFontSize": false,
            "lineHeight": false,
            "dropCap": false,
            "fontFamilies": [],
            "fontSizes": [],
            "fontStyle": false,
            "fontWeight": false,
            "letterSpacing": false,
            "textDecoration": false,
            "textTransform": false,
            "fluid": false,
            "textColumns": false
        },
        "useRootPaddingAwareAlignments": false
    }
}